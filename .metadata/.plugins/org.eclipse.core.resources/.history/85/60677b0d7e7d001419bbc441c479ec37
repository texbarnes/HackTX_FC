package assighnment6;

public class Seat {

	char row;
	char location;
	int seatNum;
	int clientNum;
	Boolean seatTaken;
	
	public Seat(char r, char l, int s)
	{
		row = r;
		location = l;
		seatNum = s;
	}
	
	public void printSeat()
	{
		System.out.println(this.row + " " + this.location + " " + this.seatNum);
	}
	
	
}
