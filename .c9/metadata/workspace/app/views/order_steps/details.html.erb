n        <p>\n            \n        </p>\n        \n        <div class=\"row\">\n         <p>\n            \n        </p>\n            <div class=\"form-group col-md-7\">\n                <%= f.label :address1, 'Address Line 1' %>\n                <%= f.text_field :address1, :class => 'form-control' %>\n            </div>\n            \n            <div class=\"form-group col-md-3\">\n                <%= f.label :city, 'City' %>\n                <%= f.text_field :city, :class => 'form-control' %>\n            </div>\n            \n            <div class=\"form-group col-md-2\">\n                <%= f.label :state, 'State' %>\n                <%= select_tag :state, options_for_select(us_states), :class => 'form-control' %>\n            </div>\n        </div>\n        \n        <div class=\"row\">\n            <div class=\"form-group col-md-7\">\n                <%= f.label :address2, 'Address Line 2 (if applicable)' %>\n                <%= f.text_field :address2, :class => 'form-control' %>\n            </div>\n            \n            <div class=\"form-group col-md-5\">\n                <%= f.label :zip, 'Zip Code' %>\n                <%= f.text_field :zip, :class => 'form-control' %>\n            </div>\n        </div>\n      \n     <div style=\"float:left;\">\n         <div class=\"col-xs-5\">\n            <a class=\"btn btn-primary\" type=\"button\" href=<%= wizard_path(:design, :order_id => @order.id) %>>Back: Design</a>\n        </div>\n     </div>  \n     <div style=\"float:right;\">\n         <div class=\"col-xs-5\">\n            <%= f.submit 'Next: Payment', :class => 'btn btn-primary' %> \n        </div>\n     </div>\n  <% end %>\n","undoManager":{"mark":86,"position":100,"stack":[[{"group":"doc","deltas":[{"start":{"row":84,"column":30},"end":{"row":84,"column":31},"action":"remove","lines":["p"]}]}],[{"group":"doc","deltas":[{"start":{"row":84,"column":29},"end":{"row":84,"column":30},"action":"remove","lines":["e"]}]}],[{"group":"doc","deltas":[{"start"